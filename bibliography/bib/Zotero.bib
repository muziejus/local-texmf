@article{kirschenbaumWhatDigitalHumanities2014,
  title = {What {{Is}} “{{Digital Humanities}},” and {{Why Are They Saying Such Terrible Things}} about {{It}}?},
  author = {Kirschenbaum, Matthew},
  date = {2014-01-01},
  journaltitle = {differences},
