@thesis{site-of-the-novel,
  type = {phdthesis},
  title = {The Site of the Novel: Objects in {{American}} Realism, 1930–1940.},
  shorttitle = {The Site of the Novel},
  author = {family=Sá Pereira, given=Moacir P., prefix=de, useprefix=false},
  date = {2014},
  institution = {University of Chicago},
  location = {Chicago},
  abstract = {The Site of the Novel attempts to reclaim a political voice for the genre of the novel. Looking at the American 1930s, the dissertation considers four literary objects, James Farrell's Studs Lonigan trilogy, John Dos Passos's U.S.A., Ernest Hemingway's For Whom the Bell Tolls and Sinclair Lewis's It Can't Happen Here. Using a contemporary arguments over speculative realist ontologies as well as the "flat ontology" suggested by site-based human geography, I argue that novels maintained their ability to work as political agents when considered beyond symptomatic readings that fix human individuals within a system of domination. Instead, entities consisting of both human and non-human actors, "sites," allow multiple, heteroglottic worlds to flourish within the novel, reflecting against the multiple worlds in which the reader finds herself. Reading this way solicits from the texts a mode of realism that reveals the experimental, creative possibilities inherent in a flattened ontology. As a result, every novel is a box of revolutionary building blocks, waiting for a new construction.},
  isbn = {9781321034141},
  langid = {english},
  pagetotal = {243 p.}
}

