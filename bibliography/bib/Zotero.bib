  shorttitle = {Hearing the Silence},
  author = {Chesnokova, Olga and Taylor, Joanna E. and Gregory, Ian N. and Purves, Ross S.},
  date = {2019-12-02},
  journaltitle = {International Journal of Geographical Information Science},
  volume = {33},
