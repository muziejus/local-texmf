@article{collectiveCounterMappingActions2012,
  title = {Counter ({{Mapping}}) {{Actions}}: {{Mapping}} as {{Militant Research}}},
  shorttitle = {Counter ({{Mapping}}) {{Actions}}},
  author = {Collective, Counter Cartographies and Dalton, Craig and Mason-Deese, Liz},
  date = {2012},
