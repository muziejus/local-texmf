@article{gilbert-naturalist-metaphors,
  title = {Naturalist Metaphors in the Literatures of {{Chicago}}, 1893–1925},
  author = {Gilbert, Emily},
  date = {1994-07-01},
  journaltitle = {Journal of Historical Geography},
  shortjournal = {Journal of Historical Geography},
  volume = {20},
  number = {3},
  pages = {283--304},
  issn = {0305-7488},
  doi = {10.1006/jhge.1994.1022},
  abstract = {At the turn of the nineteenth century Chicago experienced a literary explosion. Writers of urban fiction such as Robert Blake Fuller, Theodore Dreiser, and Upton Sinclair were trying to make sense of the modern city. In the 1920s the urban ecologists at the Chicago School of Sociology attempted to do the same. Common to both literary and theoretical discourses was the recourse to organic metaphors to describe the city. In this paper I will explore the intertextuality of these discourses. In addition, other cultural projects of the period, including the 1893 World Columbian exposition, Jane Addams' Hull House, and Daniel Burnham's 1909 Plan of the City will be considered. This paper is organized into three parts: first, I examine the points of view adopted by the urban writers; second, I explore the spatial constructions of the city; and third, I analyse the organic metaphors that predominate in these literatures. The moralism inherent to both theory and fiction is examined. I conclude with some comments and criticisms of these canonical voices, suggesting the importance of alternative visions of the city.},
  langid = {english},
  file = {/Users/moacir/Zotero/storage/H47QVS48/Gilbert - 1994 - Naturalist metaphors in the literatures of Chicago.pdf;/Users/moacir/Zotero/storage/8KE8BPRB/S030574888471022X.html}
}

@article{gilbertCounterfactualGeographiesWorlds2010,
  ids = {gilbert_counterfactual_-1,gilbert_counterfactual_2010},
  title = {Counterfactual Geographies: Worlds That Might Have Been},
  shorttitle = {Counterfactual Geographies},
  author = {Gilbert, David and Lambert, David},
  date = {2010},
  journaltitle = {Journal of Historical Geography},
  volume = {In Press, Corrected Proof},
  issn = {0305-7488},
  doi = {10.1016/j.jhg.2009.12.002},
  abstract = {This paper argues for a renewed consideration of counterfactuals within geography. Drawing upon Doreen Massey's emphasis on notions of [`]possibility', [`]chance', [`]undecidability' and [`]happenstance', we argue for an engagement with approaches in the humanities that have addressed such issues directly. We review previous uses of counterfactual method in historical geography, particularly as related to cliometrics and the [`]new economic history' of the 1960s, but argue that a recent upsurge of interest in other disciplines indicates alternative ways that [`]what-if' experiments might work in the sub-discipline. Recent counterfactual work outside of geography has had a notably spatial cast, often thinking through the nature of alternative worlds, or using counterfactual strategies that are explicitly concerned with space as well as temporal causality. We set out possible agendas for counterfactual work in historical geography. These include: consideration of the historical geographies within existing counterfactual writings and analyses; suggestions for distinctive ways that historical geographers might think and write counterfactually, including experiments in geographies of happenstance, and the exploration of more-than-human possibilities; analyses of the geography of and in counterfactual writing; and study of the political, ethical and emotional demands that counterfactuals make. This discussion and framework provides an extended introduction to this special feature on counterfactual geographies.},
  keywords = {Alternative histories,Alternative worlds,Counterfactuals,Doreen Massey,History and philosophy of geography,Speculative fiction},
