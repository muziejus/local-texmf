@article{thatcherAvoidingGhettoHope2013,
  title = {Avoiding the {{Ghetto}} through Hope and Fear: An Analysis of Immanent Technology Using Ideal Types},
  shorttitle = {Avoiding the {{Ghetto}} through Hope and Fear},
  author = {Thatcher, Jim},
  date = {2013-12-01},
  journaltitle = {GeoJournal},
